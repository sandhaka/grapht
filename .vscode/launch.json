    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [ "${command:pickArgs}" ]
        },
        {
            "name": "gen_sparse_graph",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/Tests/Scripts/gen_sparse_g.py",
            "console": "integratedTerminal",
            "args": [ "200", "3"]
        }
    ]
}